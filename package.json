{
  "name": "mocked-design-system",
  "version": "3.1.1",
  "description": "Mocked style guide design system",
  "license": "MIT",
  "repository": {},
  "scripts": {
    "dev": "concurrently  -n \"Build,SB\" \"pnpm run build --watch\" \"pnpm run storybook\" ",
    "type-check": "tsc",
    "lint": "eslint --ignore-pattern .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "pnpm run lint --fix && prettier --write .",
    "storybook": "storybook dev -p 6009",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-pattern .gitignore --fix"
    ],
    "*.{js,jsx,json,md}": "prettier --write"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@storybook/addon-a11y": "10.1.0-beta.5",
    "@storybook/addon-docs": "10.1.0-beta.5",
    "@storybook/addon-mcp": "0.1.4-next.2",
    "@storybook/addon-vitest": "10.1.0-beta.5",
    "@storybook/eslint-config-storybook": "^4.0.0",
    "@storybook/react-vite": "10.1.0-beta.5",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/browser": "3.2.3",
    "@vitest/coverage-v8": "3.2.3",
    "auto": "11.3.0",
    "concurrently": "8.0.1",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "4.4.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-storybook": "10.1.0-beta.5",
    "framer-motion": "11.0.3",
    "husky": "^8.0.0",
    "lint-staged": "16.1.0",
    "playwright": "1.53.0",
    "prettier": "3.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "10.1.0-beta.5",
    "tsup": "8.5.0",
    "typescript": "5.0.2",
    "vite": "6.3.5",
    "vitest": "3.2.3"
